{
  "name": "pocket-chat-server",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "dev": "cross-env NODE_ENV=development tsnd --respawn --transpile-only ./src/main.ts",
    "staging": "cross-env NODE_ENV=staging node ./dist/main.js",
    "start": "cross-env NODE_ENV=production node ./dist/main.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && git add -A ."
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SILVAWesley/pocket-chat-server.git"
  },
  "author": "SILVAWesley, luiznasc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SILVAWesley/pocket-chat-server/issues"
  },
  "homepage": "https://github.com/SILVAWesley/pocket-chat-server#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "node": "^17.4.0"
  }
}
